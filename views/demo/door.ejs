<div id="main" class="alt">
    <div class="inner">
        <section>
            <div class="table-wrapper">

                <table class="alt">
                    <thead>
                        <tr>
                            <th>소비저장고 아이디</th>
                            <th>열림/닫힘</th>
                            <th>시간</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for (const log of data) {
                            let door_state = "닫힘";
                            if(log.DOOR) door_state = "열림"; %>  
                        <tr>
                            <td><%= log.WAREHOUSE %></td>
                            <td><%= door_state %></td>
                            <td><%= log.TMST %></td>
                        </tr>  
                        <% } %>                      
                    </tbody>
                </table>
            </div>
        </section>
        
    </div>
</div>
